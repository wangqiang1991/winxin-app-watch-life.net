  line-height: 1.6;
  font-weight: bold;
  text-align: justify;
  color: #333;
  padding: 40rpx 0 0;
}

.entry-author {
  padding: 40rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.entry-author-info {
  display: flex;
  align-items: center;
